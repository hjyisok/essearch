{
    "name": "nansoon/essearch",
    "description": "use sql search elasticSearch",
    "type": "library",
    "license": "Apache-2.0",
    "homepage": "https://github.com/hjyisok/essearch",
    "keywords": ["sql", "elasticSearch", "dsl"],
    "authors": [
        {
            "name": "nansoon",
            "email": "nansoon@126.com"
        }
    ],
    "require": {
